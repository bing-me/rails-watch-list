<h1>This is the list view</h1>
<p><%= link_to 'add a bookmark', new_list_bookmark_path(@list.id) %></p>

<% @list.bookmarks.each do |bookmark| %>
  <p><%= bookmark.movie %>
  <%= link_to "Delete", bookmark_path(bookmark), data: {turbo_method: :delete, turbo_confirm: "Are you sure?"} %><p>
<% end %>
